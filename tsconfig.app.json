/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@reducers/*": [
        "src/app/reducers/*"
      ],
      "@actions/*": [
        "src/app/actions/*"
      ],
      "@interfaces/*": [
        "src/app/interfaces/*"
      ],
      "@services/*": [
        "src/app/services/*"
      ],
    },
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
